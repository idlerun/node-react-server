{
  "name": "node-react-server",
  "main": "index.js",
  "description": "Example NodeJS server with WebPack and ReactJS",
  "repository": "https://github.com/idlerun/node-react-server",
  "license": "BSD-2-Clause",
  "author": "Idle Run",
  "engines": {
    "node": ">=4.3.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "NODE_ENV=development node .",
    "start": "node .",
    "build": "rm -rf dist && NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "body-parser": "1.18.2",
    "copy-webpack-plugin": "4.3.1",
    "css-loader": "0.28.7",
    "express": "4.16.2",
    "fs": "0.0.2",
    "html-webpack-plugin": "2.30.1",
    "jquery": "3.2.1",
    "jsonwebtoken": "8.1.0",
    "lodash": "4.17.4",
    "log-timestamp": "0.1.2",
    "morgan": "1.9.0",
    "node-sass": "4.7.2",
    "normalize.css": "7.0.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.1",
    "webpack": "3.10.0"
  },
  "devDependencies": {
    "webpack-dev-middleware": "2.0.3",
    "webpack-hot-middleware": "2.21.0"
  }
}
